#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chris/projects/unplugin-vue-image/node_modules/.pnpm/vite@2.5.3/node_modules/vite/bin/node_modules:/Users/chris/projects/unplugin-vue-image/node_modules/.pnpm/vite@2.5.3/node_modules/vite/node_modules:/Users/chris/projects/unplugin-vue-image/node_modules/.pnpm/vite@2.5.3/node_modules:/Users/chris/projects/unplugin-vue-image/node_modules/.pnpm/node_modules:/Users/chris/projects/unplugin-vue-image/node_modules:/Users/chris/projects/node_modules:/Users/chris/node_modules:/Users/node_modules:/node_modules:/Users/chris/projects/unplugin-vue-image/playground/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chris/projects/unplugin-vue-image/node_modules/.pnpm/vite@2.5.3/node_modules/vite/bin/node_modules:/Users/chris/projects/unplugin-vue-image/node_modules/.pnpm/vite@2.5.3/node_modules/vite/node_modules:/Users/chris/projects/unplugin-vue-image/node_modules/.pnpm/vite@2.5.3/node_modules:/Users/chris/projects/unplugin-vue-image/node_modules/.pnpm/node_modules:/Users/chris/projects/unplugin-vue-image/node_modules:/Users/chris/projects/node_modules:/Users/chris/node_modules:/Users/node_modules:/node_modules:/Users/chris/projects/unplugin-vue-image/playground/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/vite@2.5.3/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/vite@2.5.3/node_modules/vite/bin/vite.js" "$@"
fi
