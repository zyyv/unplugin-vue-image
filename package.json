{
  "name": "unplugin-vue-image",
  "version": "0.1.1",
  "description": "Register global imports on demand for Vite and Webpack",
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "homepage": "https://github.com/antfu/unplugin-starter#readme",
  "bugs": {
    "url": "https://github.com/antfu/unplugin-starter/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/unplugin-starter.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./nuxt": {
      "require": "./dist/nuxt.js",
      "import": "./dist/nuxt.mjs"
    },
    "./types": {
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "lint:fix": "nr lint --fix",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@antfu/utils": "^0.5.0",
    "@rollup/pluginutils": "^4.2.0",
    "unplugin": "^0.6.0"
  },
  "devDependencies": {
    "@chris-zhu/eslint-config": "^1.0.0",
    "@types/debug": "^4.1.7",
    "@types/node": "^17.0.23",
    "bumpp": "^7.1.1",
    "debug": "^4.3.4",
    "eslint": "^8.12.0",
    "esno": "^0.14.1",
    "fast-glob": "^3.2.11",
    "local-pkg": "^0.4.1",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "tsup": "^5.12.1",
    "typescript": "^4.6.3",
    "vite": "^2.8.6",
    "vitest": "^0.8.0",
    "webpack": "^5.70.0"
  }
}
